  "devDependencies": {
    "@trendyol/jest-testcontainers": "^2.1.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cucumber": "^7.0.0",
    "@types/express": "^4.17.13",
    "@types/faker": "~5.5.9",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.10",
    "@types/pino": "^7.0.4",
    "@types/redis": "^4.0.11",
    "@types/shelljs": "^0.8.9",
    "@types/uuid": "^8.3.4",
    "@types/uuid-validate": "^0.0.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.3.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-module-resolver": "^1.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "~5.5.3",
    "husky": "^6.0.0",
    "jest": "^27.5.1",
    "jest-cucumber": "^3.0.1",
    "nodemon": "^2.0.15",
    "pino-pretty": "^7.2.0",
    "prettier": "^2.5.0",
    "shelljs": "^0.8.4",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.5.2",
    "rimraf": "^3.0.2"
  },
  "license": "ISC",
  "main": "dist/index.js",
  "name": "bank-account-manager",
  "scripts": {
    "postinstall": "npx husky install",
    "build:clean": "rm -rf dist; exit 0",
    "build": "yarn build:clean && ts-node build.ts && tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./server -o ./dist",
    "dev": "nodemon server/index.ts | pino-pretty",
    "dev:debug": "nodemon --exec \"node -r ts-node/register --inspect-brk\" server/index.ts | pino-pretty",
    "lint": "npx eslint --cache --ext .js,ts .",
    "lint:fix": "npx eslint -c .eslintrc.js \"{server, test}/**/*.{js,ts,tsx}\" --quiet --fix",
    "start": "node dist/index.js",
    "test": "yarn test:unit && yarn test:integration",
    "test:unit": "NODE_ENV=test jest --verbose",
    "test:integration": "NODE_ENV=test jest --config=jest.config.integration.js --verbose --runInBand",
    "test:features": "NODE_ENV=offline jest --verbose --runInBand --config=jest-cucumber.config.js"
  },
  "version": "1.0.0"
}
